{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"]
    },
    "outDir": "./built",
    "allowJs": true,
    "target": "es5",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "App.js"  ]
}
